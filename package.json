{
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev --turbopack",
    "build": "next build",
    "start": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "tsx prisma/seed.ts",
    "seed:admin": "curl -X POST http://localhost:3000/api/admin/seed",
    "db:seed": "prisma db seed",
    "test": "dotenv -e .env.test -- vitest",
    "test:ui": "dotenv -e .env.test -- vitest --ui",
    "test:watch": "dotenv -e .env.test -- vitest --watch",
    "test:e2e": "dotenv -e .env.test -- playwright test",
    "test:e2e:ui": "dotenv -e .env.test -- playwright test --ui",
    "test:e2e:headed": "dotenv -e .env.test -- playwright test --headed",
    "test:e2e:debug": "dotenv -e .env.test -- playwright test --debug",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "circular": "madge --circular --extensions ts,tsx .",
    "circular:image": "madge --circular --extensions ts,tsx --image graph.svg .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next": "15.5.2",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.55.1",
    "@swc/core": "^1.13.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "9",
    "eslint-config-next": "15.3.1",
    "husky": "^9.1.7",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.2.3",
    "madge": "^8.0.0",
    "next-test-api-route-handler": "^5.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.16.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "supabase"
    ],
    "onlyBuiltDependencies": [
      "supabase"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
